{
  "name": "remix-nodejs-http2-adapter",
  "version": "0.2.0",
  "type": "module",
  "source": "./index.js",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./index.js"
  },
  "files": [
    "dist/*",
    "index.js"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "microbundle --target node --format esm,cjs"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@remix-run/node": "^2"
  },
  "dependencies": {
    "mime-db": "*"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/mime-db": "^1.43.1",
    "@types/node": "^18.0.0",
    "microbundle": "^0.15.1",
    "typescript": "^5.0.2"
  }
}
