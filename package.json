{
  "name": "remix-nodejs-builtin-adapter",
  "version": "1.0.0",
  "type": "module",
  "source": "index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    "require": "dist/index.cjs",
    ".": "dist/index.mjs"
  },
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "microbundle --target node --format esm,cjs"
  },
  "peerDependencies": {
    "@remix-run/node": "^1.14.3"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "mrmime": "^1.0.1"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@tsconfig/esm": "^1.0.2",
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/strictest": "^2.0.0",
    "@types/node": "^16.18.21",
    "microbundle": "^0.15.1",
    "typescript": "^5.0.2"
  }
}
